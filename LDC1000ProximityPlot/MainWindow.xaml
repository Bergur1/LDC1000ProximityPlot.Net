<Window x:Class="LDC1000ProximityPlot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
        xmlns:prox="clr-namespace:LDC1000ProximityPlot"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <prox:MainViewModel />
    </Window.DataContext>
    <Grid>
        <Button Content="{Binding Path=ProxyButtonText}" Command="{Binding Path=ReadCommand}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75"/>
        <oxy:PlotView Model="{Binding Model}" Margin="0,37,0,0" />
    </Grid>
</Window>
